<?xml version="1.0" ?>
<robot name="my_robot">
  <!-- Laser (LiDAR) -->
  <joint name="laser_joint" type="fixed">
    <parent link="chassis"/>
    <child link="laser_frame"/>
    <origin rpy="0 0 0" xyz="-0.04625 0 0.135"/>
  </joint>
  <link name="laser_frame">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.03"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.03" radius="0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="7.583333333333335e-05" ixy="0" ixz="0" iyy="0.00012500000000000003" iyz="0" izz="7.583333333333335e-05"/>
    </inertial>
  </link>

  <gazebo reference="laser_frame">
    <material>Gazebo/Black</material>
    <sensor name="lidar" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <min_angle>-3.14159</min_angle>
            <max_angle>3.14159</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.2</min>
          <max>12</max>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_ros_laser">
        <topicName>scan</topicName>
        <frameName>laser_frame</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!-- IMU -->
  <link name="imu_link">
    <visual>
      <geometry>
        <box size="0.02 0.02 0.01"/>
      </geometry>
    </visual>
    <material name="blue">
      <color rgba="0 0 1 1.0"/>
    </material>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.01"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.01"/>
      <inertia ixx="0.00001" ixy="0" ixz="0" iyy="0.00001" iyz="0" izz="0.00001"/>
    </inertial>
  </link>

  <joint name="imu_joint" type="fixed">
    <parent link="chassis"/>
    <child link="imu_link"/>
    <origin xyz="0 0 0.05" rpy="0 0 0"/>
  </joint>

  <!--height_box = height_total(27) - wheel_radius - LiDAR-->
  <!-- Base link -->
  <link name="base_link">
  </link>
  <link name="base_footprint"/>
  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.152"/>
  </joint>
  <!-- CHASSIS -->
  <joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0 0 0"/>
  </joint>
  <link name="chassis">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.185 0.166 0.24"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.185 0.166 0.24"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.15"/>
      <inertia ixx="0.015257116666666666" ixy="0" ixz="0" iyy="0.01645197916666666" iyz="0" izz="0.011069095833333332"/>
    </inertial>
  </link>

  <!-- WHEEL LINKS -->
  <!-- FRONT WHEELS -->
  <link name="left_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>

  <link name="right_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>
  <!-- REAR WHEELS -->
  <link name="left_rear_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>

  <link name="right_rear_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>
  <!-- WHEEL JOINTS -->
  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin rpy="-1.5707963267948966 0 0" xyz="0.0925 0.0955 -0.12"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin rpy="1.5707963267948966 0 0" xyz="0.0925 -0.0955 -0.12"/>
    <axis xyz="0 0 -1"/>
  </joint>
  <joint name="left_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_rear_wheel"/>
    <origin rpy="-1.5707963267948966 0 0" xyz="-0.0925 0.0955 -0.12"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="right_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_rear_wheel"/>
    <origin rpy="1.5707963267948966 0 0" xyz="-0.0925 -0.0955 -0.12"/>
    <axis xyz="0 0 -1"/>
  </joint>

  <!-- GAZEBO -->
  <gazebo reference="laser_frame">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="chassis">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="right_front_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>0.01</mu2> 
    <kp>1000000.0</kp>
    <kd>1.0</kd>
    <minDepth>0.005</minDepth>
    <fdir1>1 0 0</fdir1>
  </gazebo>

  <gazebo reference="left_front_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>0.01</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
    <minDepth>0.005</minDepth>
    <fdir1>1 0 0</fdir1>
  </gazebo>

  <gazebo reference="left_rear_wheel">
    <material>Gazebo/Black</material>
    <mu1>50.0</mu1> 
    <mu2>50.0</mu2> 
    <kp>10000000.0</kp> 
    <kd>1.0</kd>
    <minDepth>0.005</minDepth>
    <fdir1>1 0 0</fdir1>
  </gazebo>
  
  <gazebo reference="right_rear_wheel">
    <material>Gazebo/Black</material>
    <mu1>50.0</mu1> 
    <mu2>50.0</mu2> 
    <kp>10000000.0</kp> 
    <kd>1.0</kd>
    <minDepth>0.005</minDepth>
    <fdir1>1 0 0</fdir1> 
  </gazebo>

  <gazebo reference="imu_link">
   <material>Gazebo/Blue</material>
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>true</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>imu</topicName>
        <bodyName>imu_link</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
        <initialOrientationAsReference>false</initialOrientationAsReference>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>

  <gazebo>
    <plugin filename="libgazebo_ros_diff_drive.so" name="diff_drive">
      <updateRate>50</updateRate>
      <leftJoint>left_rear_wheel_joint</leftJoint>
      <rightJoint>right_rear_wheel_joint</rightJoint>
      <wheelSeparation>0.191</wheelSeparation>
      <wheelDiameter>0.064</wheelDiameter>
      <wheelAcceleration>1.0</wheelAcceleration>
      <odometryFrame>odom</odometryFrame>
      <robotBaseFrame>base_footprint</robotBaseFrame>

      <commandTopic>cmd_vel</commandTopic>
      <odometryTopic>odom/wheel</odometryTopic> 
      
      <publishOdomTF>false</publishOdomTF> 
      
      <odometrySource>encoder</odometrySource> 
      
      <publishWheelTF>false</publishWheelTF>
      <publishOdom>true</publishOdom>
      <publishWheelJointState>false</publishWheelJointState>
      <wheelTorque>5</wheelTorque>
      <rosDebugLevel>na</rosDebugLevel>
      <publishTf>true</publishTf>
      <tf_prefix></tf_prefix>
    </plugin>

    <plugin filename="libgazebo_ros_joint_state_publisher.so" name="joint_state_publisher">
      <updateRate>50</updateRate>
      <jointName>left_front_wheel_joint,right_front_wheel_joint,left_rear_wheel_joint,right_rear_wheel_joint</jointName>
    </plugin>
  </gazebo>
</robot>