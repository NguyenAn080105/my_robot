<?xml version="1.0" ?>
<robot name="my_robot">
  <!-- Laser (LiDAR) -->
  <joint name="laser_joint" type="fixed">
    <parent link="chassis"/>
    <child link="laser_frame"/>
    <origin rpy="0 0 0" xyz="-0.04625 0 0.135"/>
  </joint>
  <link name="laser_frame">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.03"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.03" radius="0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="7.583333333333335e-05" ixy="0" ixz="0" iyy="0.00012500000000000003" iyz="0" izz="7.583333333333335e-05"/>
    </inertial>
  </link>

  <gazebo reference="laser_frame">
    <sensor name="lidar" type="ray">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>10</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>360</samples>
            <min_angle>-3.14159</min_angle>
            <max_angle>3.14159</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.15</min>
          <max>20</max>
        </range>
      </ray>
      <plugin filename="libgazebo_ros_laser.so" name="gazebo_ros_laser">
        <topicName>scan</topicName>
        <frameName>laser_frame</frameName>
      </plugin>
    </sensor>
  </gazebo>
  <!--height_box = height_total(27) - wheel_radius - LiDAR-->
  <!-- Base link -->
  <link name="base_link">
  </link>
  <link name="base_footprint"/>
  <joint name="base_footprint_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.152"/>
  </joint>
  <!-- CHASSIS -->
  <joint name="chassis_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
    <origin xyz="0 0 0"/>
  </joint>
  <link name="chassis">
    <visual>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.185 0.166 0.24"/>
      </geometry>
      <material name="grey">
        <color rgba="0.5725490196078431 0.5843137254901961 0.5686274509803921 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0"/>
      <geometry>
        <box size="0.185 0.166 0.24"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.15"/>
      <inertia ixx="0.015257116666666666" ixy="0" ixz="0" iyy="0.01645197916666666" iyz="0" izz="0.011069095833333332"/>
    </inertial>
  </link>

  <!-- WHEEL LINKS -->
  <!-- FRONT WHEELS -->
  <link name="left_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>

  <link name="right_front_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>

  <!-- REAR WHEELS -->
  <link name="left_rear_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>


  <link name="right_rear_wheel">
    <visual>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.025" radius="0.032"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.035"/>
      <inertia ixx="1.0782916666666668e-05" ixy="0" ixz="0" iyy="1.792e-05" iyz="0" izz="1.0782916666666668e-05"/>
    </inertial>
  </link>

  <!-- WHEEL JOINTS -->
  <joint name="left_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_front_wheel"/>
    <origin rpy="-1.5707963267948966 0 0" xyz="0.0925 0.0955 -0.12"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="right_front_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_front_wheel"/>
    <origin rpy="1.5707963267948966 0 0" xyz="0.0925 -0.0955 -0.12"/>
    <axis xyz="0 0 -1"/>
  </joint>
  <joint name="left_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_rear_wheel"/>
    <origin rpy="-1.5707963267948966 0 0" xyz="-0.0925 0.0955 -0.12"/>
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="right_rear_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_rear_wheel"/>
    <origin rpy="1.5707963267948966 0 0" xyz="-0.0925 -0.0955 -0.12"/>
    <axis xyz="0 0 -1"/>
  </joint>
  
  <!-- GAZEBO -->
  <gazebo reference="laser_frame">
    <material>Gazebo/Black</material>
  </gazebo>

  <gazebo reference="chassis">
    <material>Gazebo/Blue</material>
  </gazebo>

  <gazebo reference="right_front_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <gazebo reference="left_front_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <gazebo reference="left_rear_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>
  
  <gazebo reference="right_rear_wheel">
    <material>Gazebo/Black</material>
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <kp>1000000.0</kp>
    <kd>1.0</kd>
  </gazebo>

  <gazebo>
    <!-- GAZEBO ROS CONTROL -->
    <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
      <robotNamespace>/</robotNamespace>
    </plugin>
  </gazebo>

  <!-- TRANSMISSION (ROS Control) >
  <transmission name="left_front_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_front_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="left_front_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="right_front_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="right_front_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="right_front_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission-->

  <transmission name="left_rear_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_rear_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="left_rear_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="right_rear_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="right_rear_wheel_joint">
      <hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
    </joint>
    <actuator name="right_rear_motor">
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>
</robot>